{
  "trailingSlash": false,
  "buildCommand": "echo 'No build needed - static files'",
  "outputDirectory": ".",
  "installCommand": "echo 'No install needed'",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOWALL"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}